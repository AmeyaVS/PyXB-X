mkdir -p bindings
rm bindings/*.wxs

# Some must be done in order, to handle dependencies
python storewxs.py schemas/xml.xsd bindings/xml.wxs
python storewxs.py schemas/XMLSchema.xsd bindings/xsd.wxs
python storewxs.py schemas/XMLSchema-hasFacetAndProperty.xsd bindings/xsd-hfp.wxs
python storewxs.py schemas/wsdl.xsd bindings/wsdl.wxs
python storewxs.py schemas/xhtml.xsd bindings/xhtml.wxs

# Process anything remaining
python storewxs.py schemas/kml21.xsd bindings/kml21.wxs
python storewxs.py schemas/xmldsig-core-schema.xsd bindings/xmldsig.wxs
python storewxs.py schemas/xenc-schema.xsd bindings/xenc.wxs

python storewxs.py schemas/saml-schema-assertion-2.0.xsd bindings/saml-assert.wxs
