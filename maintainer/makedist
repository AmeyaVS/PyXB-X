#!/bin/sh

DISTDIR=pyxbdist
cd /tmp
rm -rf ${DISTDIR}
svn export svn://scmhome/pyxb/trunk ${DISTDIR}
cd ${DISTDIR}
cd doc
rm -rf html
rm -f pyxb
ln -s ../pyxb .
#make apihtml
#make  html
rm -f pyxb
cd ..
for bundle in core wssplat ; do
  pyxb/bundles/${bundle}/scripts/genbind || exit 1
done
./setup.py sdist
./setup.py test
cp -p dist/PyXB-*.tar.gz ~/pyxb/pre-release
